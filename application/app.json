{
  "expo": {
    "name": "Sapling",
    "slug": "Sapling",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {},
    "ios": {
      "bundleIdentifier": "com.nick-hulley.Sapling",
      "supportsTablet": true,
      "infoPlist": {
        "NSAllowsArbitraryLoads": true,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "description": ""
  }
}
